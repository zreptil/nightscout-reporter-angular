{
  "name": "nightscout-reporter",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3002 --open",
    "backend": "cmd /c backend.bat",
    "pre-build": "node tools/update-version.js",
    "start-demo": "ng serve --configuration demo --port 3003 --open",
    "build": "npm run pre-build && ng build && npm run zip && npm run rename",
    "build-dev": "npm run pre-build && ng build --configuration dev && npm run zip && npm run rename",
    "build-demo": "npm run pre-build && ng build --configuration demo",
    "zip": "node tools/prepare-zip.js && npm-build-zip --source=dist/nightrep/zip --destination=dist/nightrep",
    "rename": "node -e \"require('fs').rename('dist/nightrep/nightscout-reporter_5.0.0.zip', 'dist/nightrep/browser/nightscout-reporter_local.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "watch": "npm run pre-build && ng build --watch --configuration development",
    "i18n-01-extract": "ng extract-i18n --output-path src/locale --out-file messages.de-DE.xliff --format xliff --progress",
    "i18n-02-upload-german": "start https://crowdin.com/project/nightrep/content/files",
    "i18n-03-create-english-source": "tsc tools/i18n-create-english.ts && node tools/i18n-create-english.js",
    "i18n-04-upload-english": "start https://crowdin.com/project/nightrep-english/content/files",
    "i18n-12-extract-and-upload-german": "npm run i18n-01-extract && npm run i18n-02-upload-german",
    "i18n-34-create-and-upload-english": "npm run i18n-03-create-english-source && npm run i18n-04-upload-english",
    "i18n-05-create-messages": "tsc tools/i18n-create-messages.ts && node tools/i18n-create-messages.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.7",
    "@angular/compiler": "^19.1.7",
    "@angular/core": "^19.1.7",
    "@angular/forms": "^19.1.7",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@types/google-one-tap": "^1.2.2",
    "@types/pdfmake": "^0.2.1",
    "@types/ws": "^8.5.4",
    "base64-arraybuffer": "^1.0.2",
    "better-npm-audit": "^3.7.3",
    "extract-zip": "^2.0.1",
    "fast-sha256": "^1.3.0",
    "file-saver": "^2.0.5",
    "google-one-tap": "^1.0.6",
    "jszip": "^3.10.1",
    "node-ensure": "^0.0.0",
    "pdfjs-dist": "^4.2.67",
    "pdfmake": "^0.2.7",
    "rxjs": "~7.8.0",
    "sha1": "^1.1.1",
    "tslib": "^2.3.0",
    "utf8-encoding": "^0.1.2",
    "xliff": "^6.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.7",
    "@angular/localize": "^19.1.7",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^12.11.1",
    "@types/pdfjs-dist": "^2.10.378",
    "fs-extra": "^11.3.0",
    "jasmine-core": "~4.5.0",
    "npm-build-zip": "^1.0.3",
    "typescript": "~5.5.4"
  }
}