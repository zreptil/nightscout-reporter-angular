{
  "name": "nightscout-reporter",
  "version": "4.4.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3002 --open",
    "start-demo": "ng serve --configuration demo --port 3003 --open",
    "build": "ng build && npm run zip",
    "build-dev": "ng build --configuration dev && npm run zip",
    "build-demo": "ng build --configuration demo",
    "zip": "npm-build-zip --source=dist/nightrep/browser --destination=dist/nightrep/browser --name=nightscout-reporter_local --name_only=true",
    "watch": "ng build --watch --configuration development",
    "pdfmake-font-generator": "pdfmakefg src/assets/fonts/ttf src/assets/fonts/created.js",
    "i18n-01-extract": "ng extract-i18n --output-path src/locale --out-file messages.de-DE.xliff --format xliff --progress",
    "i18n-02-upload-german": "start https://crowdin.com/project/nightrep/content/files",
    "i18n-03-create-english-source": "tsc tools/i18n-create-english.ts && node tools/i18n-create-english.js",
    "i18n-04-upload-english": "start https://crowdin.com/project/nightrep-english/content/files",
    "i18n-12-extract-and-upload-german": "npm run i18n-01-extract && npm run i18n-02-upload-german",
    "i18n-34-create-and-upload-english": "npm run i18n-03-create-english-source && npm run i18n-04-upload-english",
    "i18n-05-create-messages": "tsc tools/i18n-create-messages.ts && node tools/i18n-create-messages.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.1",
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.1",
    "@angular/compiler": "^20.0.1",
    "@angular/core": "^20.0.1",
    "@angular/forms": "^20.0.1",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.1",
    "@angular/platform-browser-dynamic": "^20.0.1",
    "@angular/router": "^20.0.1",
    "@types/google-one-tap": "^1.2.2",
    "@types/pdfmake": "^0.2.7",
    "angular-oauth2-oidc": "^19.0.0",
    "base64-arraybuffer": "^1.0.2",
    "better-npm-audit": "^3.7.3",
    "extract-zip": "^2.0.1",
    "fast-sha256": "^1.3.0",
    "file-saver": "^2.0.5",
    "google-one-tap": "^1.0.6",
    "jszip": "^3.10.1",
    "node-ensure": "^0.0.0",
    "pdfjs-dist": "^4.2.67",
    "pdfmake": "^0.2.20",
    "rxjs": "~7.8.0",
    "sha1": "^1.1.1",
    "tslib": "^2.3.0",
    "utf8-encoding": "^0.1.2",
    "xliff": "^6.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.1",
    "@angular/localize": "^20.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.5.0",
    "npm-build-zip": "^1.0.3",
    "pdfmake-font-generator": "^1.0.1",
    "typescript": "~5.8.3"
  }
}
