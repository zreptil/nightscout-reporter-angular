{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "newProjectRoot": "projects",
  "projects": {
    "nightscout-reporter": {
      "i18n": {
        "sourceLocale": "de"
      },
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "anyComponentStyle"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/web.config",
                {
                  "input": "src/build-replacements/assets/master",
                  "output": "assets/",
                  "glob": "*.json"
                },
                {
                  "input": "src/build-replacements/assets/master/php",
                  "output": "assets/php/",
                  "glob": "*.php"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false
            },
            "demo": {
              "outputPath": {
                "base": "demo"
              },
              "budgets": [
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "anyComponentStyle"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/web.config",
                {
                  "input": "src/build-replacements/assets/demo",
                  "output": "assets/",
                  "glob": "*.json"
                },
                {
                  "input": "src/build-replacements/assets/demo/php",
                  "output": "assets/php/",
                  "glob": "*.php"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.demo.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false
            },
            "dev": {
              "budgets": [
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10mb",
                  "maximumWarning": "8mb",
                  "type": "anyComponentStyle"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/web.config",
                {
                  "input": "src/_php/src",
                  "output": "backend/",
                  "glob": "**/*.php"
                },
                {
                  "input": "src/_php/src",
                  "output": "backend/",
                  "glob": ".htaccess"
                },
                {
                  "input": "src/_php/src",
                  "output": "backend/",
                  "glob": "web.config"
                },
                {
                  "input": "src/build-replacements/assets/dev",
                  "output": "assets/",
                  "glob": "*.json"
                },
                {
                  "input": "src/build-replacements/assets/dev/php",
                  "output": "assets/php/",
                  "glob": "*.php"
                },
                {
                  "input": "src/build-replacements/_php/config/apps",
                  "output": "backend/config/apps",
                  "glob": "*.php"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "allowedCommonJsDependencies": [
              "jszip",
              "pdfmake",
              "file-saver",
              "pdfjs-dist",
              "fast-sha256",
              "node-ensure",
              "https://apis.google.com/js/api.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/web.config"
            ],
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "outputPath": {
              "base": "dist/nightrep"
            },
            "polyfills": [
              "src/polyfills.ts"
            ],
            "scripts": [
              "src/assets/scripts/pdf.js",
              "src/assets/scripts/pdf.worker.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json",
            "browser": "src/main.ts"
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "nightscout-reporter:build"
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "nightscout-reporter:build:development"
            },
            "production": {
              "buildTarget": "nightscout-reporter:build:production"
            },
            "demo": {
              "buildTarget": "nightscout-reporter:build:demo"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "inlineStyleLanguage": "scss",
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "module": "app",
          "skipTests": true,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "module": "app",
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "module": "app",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      }
    }
  },
  "version": 1,
  "cli": {
    "analytics": "30a9b09d-7682-4302-916b-8e3f7c176ff9"
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
